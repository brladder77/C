#include <stdio.h>

int main()
{
	printf("[%-5d]\n", 23);

	int a = 5, b = 7;
	printf("%d", ++a);
	printf("%d\n", --b);

	a = 10, b = 20;
	int c = 3;
	c = (a++ == 11) && (b-- == 20);
	printf("%d\n", c);

	printf("%d\n", 0 & 1);

	int i = 1, sum = 0;
	
	while (i <= 10) {
		sum = sum + i;
		i = i + 1;
	}
	printf("%d\n", sum);
	printf("%d\n", ~3);
	printf("%d\n", 10 << 2);

	int t=4, j;
	printf("4ë‹¨\n");
	for (j = 1; j <= 9; j++)
		printf("%d x %d = %d\n", t, j, t*j);
	printf("\n");
	
	return 0;

}
