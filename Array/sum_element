#include <stdio.h>

int main() {

	int Arr[2][3] = { 1,3,5,7,9,11 };
	int i, j, sum = 0, y = 0;
  
	for (i = 0; i < 2; i++) {
		for (j = 0; j < 3; j++) {
			sum = sum + Arr[i][j];
			y++;
		}
	}
  
	float ave;
	ave = float(sum) / y;
	printf("각 요소의 합은 %d 이고, 평균은 %.2f 입니다.\n", sum, ave);
  
  return 0;
}
